{
    "manifest_version": 3,
    "name": "MapMe",
    "description": "Chrome extension that uses AI to generate a map from a website's source code",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/mapme-icon-48.png"
    },
    "icons": {
        "16": "images/mapme-icon-16.png",
        "32": "images/mapme-icon-32.png",
        "48": "images/mapme-icon-48.png",
        "128": "images/mapme-icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ]
}